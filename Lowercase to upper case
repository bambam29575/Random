#include <stdio.h>
#include <ctype.h>


int main() {
   char ch[50];
   scanf("%s", ch);
for(int i = 0; ch[i] != '\0'; i++){
        if(islower(ch[i])){
            ch[i] = toupper(ch[i]);
        }else if(isupper(ch[i])){
            ch[i] = tolower(ch[i]);
        }
    }printf("%s",ch);
   

    return 0;
}
